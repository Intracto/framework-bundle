<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
    	<parameter key="kingfoo_framework.validator.mapping_dirs" type="collection" />
    </parameters>

    <services>
    	<service id="kingfoo_framework.monolog.psr_log_message_processor" class="Monolog\Processor\PsrLogMessageProcessor" public="false">
    		<tag name="monolog.processor" />
    	</service>
    </services>
</container>
